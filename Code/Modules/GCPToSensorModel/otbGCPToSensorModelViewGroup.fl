          callback {if(cDEM->value()==0)
{
cDEM->value(1);
}
else
{
cMean->value(0);
cElev->value(0);
bDEMBrowser->activate();
tDEMFile->activate();
vMeanElev->deactivate();
vElev->deactivate();
}}
          xywh {15 30 15 15} down_box ROUND_DOWN_BOX color 7
