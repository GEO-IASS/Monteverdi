        xywh {10 123 300 30} box PLASTIC_DOWN_BOX color 7 labelcolor 186 align 5
      } {
        Fl_Round_Button cMean {
          callback {if(cMean->value()==0)
{
cMean->value(1);
}
else
{
cDEM->value(0);
cElev->value(0);
bDEMBrowser->deactivate();
tDEMFile->deactivate();
vMeanElev->activate();
vElev->deactivate();
}}
          xywh {15 131 15 15} down_box ROUND_DOWN_BOX color 7
