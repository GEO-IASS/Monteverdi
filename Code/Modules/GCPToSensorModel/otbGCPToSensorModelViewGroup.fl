        label {GCPs elevation} open
        xywh {10 182 300 30} box PLASTIC_DOWN_BOX color 7 labelcolor 186 align 5
      } {
        Fl_Round_Button cElev {
          callback {if(cElev->value()==0)
{
cElev->value(1);
}
else
{
cDEM->value(0);
cMean->value(0);
bDEMBrowser->deactivate();
tDEMFile->deactivate();
vMeanElev->deactivate();
vElev->activate();
}}
          xywh {15 189 15 15} down_box ROUND_DOWN_BOX value 1 color 7
