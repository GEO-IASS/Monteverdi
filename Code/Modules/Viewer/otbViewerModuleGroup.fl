  Function {BuildDEM()} {open return_type {virtual void}
  } {
    Fl_Window wDEM {
      label {DEM Selection}
      xywh {717 527 390 100} type Double box PLASTIC_DOWN_BOX color 23 labelfont 1 labelsize 12 labelcolor 186 resizable visible
    } {
      Fl_Check_Button gUseInputDEM {
        label {Use DEM for Loading}
        callback {if( gUseInputDEM->value()==1 || gUseOutputDEM->value()==1 )
{
gLoadDEM->activate();
gDEMPath->activate();
}
else
{
gLoadDEM->deactivate();
gDEMPath->deactivate();
}}
        xywh {15 14 64 15} down_box DOWN_BOX labelsize 12
      }
      Fl_Check_Button gUseOutputDEM {
        label {Use DEM for Processing}
        callback {if( gUseInputDEM->value()==1 || gUseOutputDEM->value()==1 )
{
gLoadDEM->activate();
gDEMPath->activate();
}
else
{
gLoadDEM->deactivate();
gDEMPath->deactivate();
}}
        xywh {220 14 64 15} down_box DOWN_BOX labelsize 12 hide deactivate
      }
      Fl_Button gLoadDEM {
        label Load
        callback {const char * cfname = fl_dir_chooser("Choose DEM Directory...", "");
Fl::check();
wDEM->redraw();
gDEMPath->value(cfname);
gDEMPath->redraw();}
        xywh {16 38 64 25} box PLASTIC_DOWN_BOX down_box PLASTIC_DOWN_BOX color 15 labelfont 1 deactivate
      }
      Fl_Output gDEMPath {
        xywh {87 38 288 25} box PLASTIC_DOWN_BOX align 0 deactivate
      }
      Fl_Button gQuitDEM {
        label Quit
        callback {wDEM->hide();}
        xywh {311 70 64 25} box PLASTIC_DOWN_BOX down_box PLASTIC_DOWN_BOX color 90 labelfont 1
      }
      Fl_Button gOKDEM {
